<Window x:Class="TXM.GUI.Dialogs.OutputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Output" Height="400" Width="600" SizeChanged="Window_SizeChanged">
    <Grid>
        <TextBlock x:Name="textblockOutput" TextWrapping="Wrap" Text="" Margin="0,0,0,25"/>
        <Button x:Name="ButtonCopy" Content="Copy" Margin="0,0,90,0" HorizontalAlignment="Right" Width="75" Height="20" VerticalAlignment="Bottom" Click="Button_Click_1"/>
        <Button x:Name="ButtonClose" Content="Close" Margin="0,0,10,0" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
